    "react-hook-form": "^7.29.0",
    "react-modal": "^3.15.1",
    "react-refresh": "^0.11.0",
    "react-router-dom": "^6.2.2",
    "resolve": "^1.20.0",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^12.3.0",
